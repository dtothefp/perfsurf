<h1>Locations Index for Users</h1>

<ul>
  <% @locations.each do |location| %>
    <li><%= link_to location.name, location_path(location) %></li>
    <li><%= location.msw_id %></li>
  <% end %>
</ul>

<div id="map" style="height:400px; width:800px;"></div>

<img src='<%= asset_path "usa-map.svg" %>' >

<script>
window.onload = function(){
  console.log("loaded");
  var map = kartograph.map('#map', 600, 400);
  map.loadMap('<%= asset_path "usa-map.svg" %>', function() {
    console.log("load map");
  });
}


</script>